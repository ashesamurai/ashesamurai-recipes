<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Downloads the latest x86_64 build of Microsoft Edge</string>
    <key>Identifier</key>
    <string>com.github.ashesamurai.download.x86_64.msedge</string>
    <key>Input</key>
    <dict>
        <key>NAME</key>
        <string>Edge</string>
        <key>ARCHITECTURE</key>
        <string>x86_64</string>
        <key>CHANNEL</key>
        <string>Stable</string>
        <key>LINKID</key>
        <string>2069148</string>
        <key>DOWNLOAD_URL</key>
        <string>https://go.microsoft.com/fwlink/?linkid=%LINKID%&platform=Mac&Consent=0&channel=%CHANNEL%</string>
    </dict>
    <key>MinimumVersion</key>
    <string>2.3</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Processor</key>
            <string>URLDownloader</string>
            <key>Arguments</key>
            <dict>
                <key>url</key>
                <stirng>%DOWNLOAD_URL%</string>
                <key>filename</key>
                <string>%NAME%.%ARCHITECTURE%.pkg</string>
            </dict>
        </dict>
        <dict>
            <key>Processor</key>
            <string>EndOfCheckPhase</string>
        </dict>
        <dict>
            <key>Processor</key>
            <string>CodeSignatureVerifier</string>
            <key>Arguments</key>
            <dict>
                <key>input_path</key>
                <string>%pathname%</string>
                <key>expected_authority_names</key>
                <array>
                    <string>Developer ID Installer: Microsoft Corporation (UBF8T346G9)</string>
                    <string>Developer ID Certification Authority</string>
                    <string>Apple Root CA</string>
                </array>
            </dict>
        </dict>
    </array>
</dict>
</plist>